{
  "manifest_version": 3,
  "name": "Limpar Cache",
  "version": "1.0",
  "description": "Extensão para limpar cache e dados de navegação",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "browsingData",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
